#!/usr/bin/python3
import json
from PIL import Image, ImageDraw, ImageFont

imgpath = "img/drake_800x800.png"
outputname = "meme.png"

fn = "img/drake_800x800.json"
data = open(fn).read()
data = json.loads(data)

ta = data['text_areas']

for a in ta:
    print(a)

image = Image.open(imgpath)

image.save(outputname)
